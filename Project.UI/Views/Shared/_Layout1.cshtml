                                                            @foreach (var item in ViewBag.Models)
                                                            {

                                                                <li style="margin-bottom:13px;">
                                                                    <ul>

                                                                        <a asp-action="Products" asp-controller="Home" asp-route-modelId="@item.Id">
                                                                            @item.Name

