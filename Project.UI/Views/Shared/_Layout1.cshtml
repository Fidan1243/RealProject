                                    <ul>
                                        <li>
                                            <a href="/Home/Index">Home</a>
                                        </li>
                                        <li class="has-dropdown has-megaitem">
                                            <a href="/Home/Products">
                                                Products <i class="fa fa-angle-down"></i>

                                            </a>
                                            <!-- Mega Menu -->
                                            <div class="mega-menu">
                                                <ul class="mega-menu-inner">
                                                    <!-- Mega Menu Sub Link -->
                                                    <li class="mega-menu-item">
                                                        <a href="#" class="mega-menu-item-title">Products</a>
                                                        <ul class="mega-menu-sub">
                                                            @foreach (var item in ViewBag.Models)
                                                            {

                                                                <li style="margin-bottom:13px;">
                                                                    <ul>

                                                                        <a asp-action="Products" asp-controller="Home" asp-route-modelId="@item.Id">
                                                                            @item.Name

                                                                        </a>
                                                                    </ul>
                                                                    <ul>

                                                                        @if (ViewBag.Role == "Admin")
                                                                        {

                                                                            <a asp-action="UpdateModel" asp-controller="Admin" asp-route-modelId="@item.Id">
                                                                                Update
                                                                            </a>

                                                                            <a asp-action="DeleteModel" asp-controller="Admin" asp-route-modelId="@item.Id">
                                                                                Delete
                                                                            </a>
                                                                        }
                                                                    </ul>

                                                                </li>
                                                            }
                                                            @if (ViewBag.Role == "Admin")
                                                            {
                                                                <li>

                                                                    <a asp-action="AddModel" asp-controller="Admin">
                                                                        Add Model
                                                                    </a>

                                                                </li>

                                                            }
                                                        </ul>
                                                    </li>
                                                    <!-- Mega Menu Sub Link -->
                                                    <li class="mega-menu-item">
                                                        <ul class="mega-menu-sub">
                                                            @foreach (var item in ViewBag.Materials)
                                                            {

                                                                <li style="margin-bottom:13px;">
